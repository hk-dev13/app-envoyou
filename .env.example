# Envoyou App Dashboard Environment Variables Template

# API Configuration
VITE_API_BASE_URL=https://api.envoyou.com
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=Envoyou Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=User Dashboard for Envoyou API Management
VITE_APP_ENV=development

# Authentication
VITE_JWT_EXPIRES_IN=7d
VITE_SESSION_TIMEOUT=3600000

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_API_MOCKING=false
VITE_SHOW_API_TESTER=true

# External Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_STRIPE_PUBLISHABLE_KEY=

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
